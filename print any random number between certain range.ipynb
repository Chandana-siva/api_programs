    "import random\n",
    "from flask import Flask,request\n",
    "app=Flask(__name__)\n",
    "@app.route(\"/random_number\",methods=[\"POST\"])\n",
    "def ch():\n",
    "    a=int(request.form.get(\"a\"))\n",
    "    b=int(request.form.get(\"b\"))\n",
    "    return str(random.randint(a,b))\n",
    "if __name__==\"__main__\":\n",
    "    app.run(port=1233)"
   ]
  }
